prefix=${pcfiledir}/../..

Name: xenplat
Version: 0.10
URL: http://wiki.xen.org/wiki/Unikraft
Description: Xen platform support via Unikraft
Cflags: -isystem${prefix}/include/xenplat -isystem${prefix}/include/xenplat/plat/common/include -isystem${prefix}/include/xenplat/plat/xen/include -isystem${prefix}/include/xenplat/unikraft-misc-includes @ARCH_CFLAGS@ -fno-builtin-printf -nostdinc -isystem@GCC_INSTALL@include -U__linux__ -U__FreeBSD__ -U__sun__ -U__linux -D__MINIOS__ -D__X86_64__
Libs: -L${prefix}/lib/xenplat -lxenplat -T${prefix}/lib/xenplat/link64.lds @ARCH_LDFLAGS@
